	style(type='text/css').
		@-webkit-keyframes dissapear {
			0% {
				opacity: 1;
				visibility: visible;
			}
			100% {
				opacity: 0;
				visibility: hidden;
			}
		}
		@keyframes dissapear {
			0% {
				opacity: 1;
				visibility: visible;
			}
			100% {
				opacity: 0;
				visibility: hidden;
			}
		}
		body.content-loaded #loader {
			-webkit-animation: dissapear .75s ease forwards;
			        animation: dissapear .75s ease forwards;
			-webkit-transform: translate3d(0,0,0);
			        transform: translate3d(0,0,0);
		}
		#loader {
			background-color: #0e1a35;
			position: fixed;
			width: 100%;
			height: 100%;
			top: 0;
			left: 0;
			z-index: 10000000;
			display: -webkit-box;
			display: -ms-flexbox;
			display: flex;
			-webkit-box-pack: center;
			    -ms-flex-pack: center;
			        justify-content: center;
			-webkit-box-align: center;
			    -ms-flex-align: center;
			        align-items: center;
		}

		#loader .gif {
			background-repeat: none;
			width: 351px;
			height: 300px;
		}